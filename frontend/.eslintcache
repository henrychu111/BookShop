exact component={HomeScreen} />\n        </div> \n    </main>\n    <footer className=\"page-footer\">\n        <div>\n            All rights reserved\n        </div>\n    </footer>\n</div>\n</BrowserRouter>\n  );\n}\n\nexport default App;\n","C:\\Users\\henry\\Desktop\\BCIT Documents\\Henry's Website\\BookShop\\frontend\\src\\Screens\\HomeScreen.js",["91","92"],"import React, {useState, useEffect} from 'react';\r\nimport {Link} from 'react-router-dom'\r\nimport {useSelector, useDispatch} from 'react-redux'\r\nimport { listProducts } from '../actions/productActions';\r\nimport Rating from '@material-ui/lab/Rating';\r\nimport Tooltip from '@material-ui/core/Tooltip';\r\n\r\nfunction HomeScreen (props) {\r\n    const productList = useSelector(state => state.productList);\r\n    const {books, loading, error} = productList;\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        dispatch(listProducts());\r\n\r\n        return () => {\r\n        };  \r\n    }, [])\r\n\r\n    return  loading? <div className=\"loading\">Loading...</div>:\r\n        error? <div>{error}</div>:\r\n\r\n            <div><ul className=\"products\">\r\n            {\r\n                books.map(book =>   \r\n            <li key ={book._id}>\r\n                <div className=\"product\">\r\n                    <Link to={'/book/' + book._id}>\r\n                        {<img className=\"product-image\" src={book.image} alt=\"product\"/>}\r\n                    </Link>\r\n                    \r\n                    <div className=\"product-name\"> \r\n                        <Link to={'/book/' + book._id}>{book.name}</Link>\r\n                    </div>\r\n                    <div className=\"product-author\">{book.author}</div>\r\n                    <div className=\"product-type\">{book.type}</div>\r\n                    <div className=\"product-price\">${book.price}</div>\r\n                    <div className=\"product-rating\"><Tooltip className=\"tooltip\" title={book.rati